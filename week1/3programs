TO ADD TWO COMPLEX NUMBERS
#include<stdio.h>
typedef struct
{
    float real;
    float imag;
} complex;
complex add(complex r1,complex r2)
{
    complex res;
    res.real=r1.real+r2.real;
    res.imag=r1.imag+r2.imag;
    return res;
}
int main()
{
    complex r1={4,6};
    complex r2={1,3};
    complex r3=add(r1,r2);
    printf("result:%f+i.%f\n",r3.real,r3.imag);
    return 0;
}


TO ADD TWO NUMBERS
#include<stdio.h>
int add(int a,int b)
{
    return a+b;
}
int main()
{
    int a=10;
    int b=5;
    int c=add(a,b);
    printf("result is:%d\n",c);
    return 0;
}

TO ADD THREE NUMBERS
#include<stdio.h>
int add(int a,int b,int c)
{
    return a+b+c;
}
int main()
{
    int a=10;
    int b=5;
    int c=5;
    int d=add(a,b,c);
    printf("result is:%d\n",d);
    return 0;
}
 
